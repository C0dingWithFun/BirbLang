class Test {
    String name = "";
    int age = 0;

    void Test(String name, int age) {
        nest.name = name;
        nest.age = age;
    }

    int getAge() {
        return nest.age;
    }

    String getName() {
        return nest.name;
    }
}

class test = Test;
test.Test("Birb", 10);

assert(test.getName() == "Birb" && test.getAge() == 10);
screm(test.getName() + " : " + test.getAge());